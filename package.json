        "prettier": "^2.6.2",
        "prettier-plugin-prisma": "^3.14.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.2.3",
        "ts-jest": "28.0.3",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "^4.7.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "engines": {
        "node": "16.x",
        "npm": "8.x"
    }
