        "cli-progress": "^3.11.1",
        "config": "^3.3.7",
        "consola": "^2.15.3",
        "csv": "^6.1.0",
        "dotenv": "^16.0.1",
        "dotenv-flow": "^3.2.0",
        "fs-extra": "^10.1.0",
        "i18n": "^0.15.0",
        "moment": "^2.29.3",
        "morgan": "^1.10.0",
        "nest-morgan": "^2.0.0",
        "nestjs-command": "^3.1.1",
        "node-random-name": "^1.0.1",
        "prisma": "^3.14.0",
